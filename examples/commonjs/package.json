{
  "name": "nestjs-piscina-commonjs-basic-example",
  "version": "1.0.0",
  "description": "Basic TypeScript example using nestjs-piscina with CommonJS output",
  "main": "dist/app.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/app.js",
    "dev": "node -r ts-node/register/transpile-only src/app.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "nestjs-piscina": "../../nestjs-piscina.tgz",
    "piscina": "^5.1.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.x"
  },
  "author": "NestJS Piscina Examples",
  "license": "MIT"
