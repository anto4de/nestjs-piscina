{
  "name": "nestjs-piscina-esm-basic-example",
  "version": "1.0.0",
  "description": "Basic TypeScript example using nestjs-piscina with ESM output",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/app.js",
    "dev": "node --loader ts-node/esm/transpile-only src/app.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "nestjs-piscina": "../../nestjs-piscina-1.0.0.tgz",
    "piscina": "^4.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.x"
  },
  "author": "NestJS Piscina Examples",
  "license": "MIT"
}
